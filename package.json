{
  "name": "mcgregor-codes",
  "version": "2.2.0",
  "description": "McGregor Codes portfolio website",
  "keywords": [
    "react",
    "js",
    "javascript",
    "gulp",
    "browserify"
  ],
  "main": "src/js/index.js",
  "scripts": {
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcgregor/mcgregor.codes"
  },
  "author": "ianmcgregor",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "boid": "^0.2.4",
    "dat-gui": "^0.5.0",
    "events": "^1.1.1",
    "flux": "^2.1.1",
    "gsap": "^1.19.0",
    "history": "^2.0.0-rc2",
    "lodash": "^4.15.0",
    "normalize.css": "^4.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-mixin": "^3.0.5",
    "react-router": "^2.7.0",
    "suitcss-base": "^2.0.0",
    "usfl": "^2.0.6"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.14.0",
    "browserify": "^13.1.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.3.0",
    "cssnano": "^3.7.4",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "esprima-fb": "^15001.1001.0-dev-harmony-fb",
    "gulp": "^3.9.1",
    "gulp-debug": "^2.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-if": "^2.0.1",
    "gulp-image-resize": "^0.10.0",
    "gulp-imagemin": "^3.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^6.1.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-template": "^4.0.0",
    "gulp-uglify": "^2.0.0",
    "history": "^3.0.0",
    "imagemin-webp": "^4.0.0",
    "karma": "^1.2.0",
    "karma-browserify": "^5.1.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "mocha": "^3.0.2",
    "postcss-bem-linter": "^2.5.1",
    "postcss-calc": "^5.3.1",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-easings": "^0.3.0",
    "postcss-import": "^8.1.2",
    "postcss-nested": "^1.0.0",
    "postcss-url": "^5.1.2",
    "sitemap": "^1.8.1",
    "stylelint": "^7.1.0",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "yargs": "^5.0.0"
  },
  "browser": {},
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-0",
            "react"
          ]
        }
      ]
    ]
  },
  "paths": {
    "connect": {
      "dirMin": "./",
      "dir": "dist",
      "files": [
        "dist/**/*",
        "index.html"
      ]
    },
    "html": {
      "entry": "src/index.html",
      "dest": "dist",
      "destMin": ""
    },
    "images": {
      "entry": "src/img/png",
      "dest": "dist/img"
    },
    "scripts": {
      "entry": "src/index.js",
      "dest": "dist/js",
      "modernizr": {
        "entry": "src/utils/modernizr.js",
        "dest": "dist/js"
      },
      "lint": [
        "src/**/*.js",
        "tasks/**/*.js",
        "!src/utils/modernizr.js"
      ]
    },
    "styles": {
      "entry": "src/index.css",
      "dest": "dist/css/",
      "watch": [
        "src/**/*.css"
      ],
      "lint": [
        "src/**/*.css",
        "!src/font/**/*.css"
      ]
    }
  }
}
