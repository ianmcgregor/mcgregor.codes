{
  "name": "mcgregor-codes",
  "version": "2.2.0",
  "description": "McGregor Codes portfolio website",
  "keywords": [
    "react",
    "js",
    "javascript",
    "gulp",
    "browserify"
  ],
  "main": "src/js/index.js",
  "scripts": {
    "test": "karma start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ianmcgregor/mcgregor.codes"
  },
  "author": "ianmcgregor",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "boid": "^0.2.2",
    "dat-gui": "^0.5.0",
    "events": "^1.1.0",
    "flux": "^2.1.1",
    "history": "^2.0.0-rc2",
    "lodash": "^3.10.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-mixin": "^3.0.3",
    "react-router": "^1.0.3",
    "suitcss-base": "^1.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-eslint": "^5.0.0-beta6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.11.0",
    "browserify": "^13.0.0",
    "chai": "^3.4.1",
    "chalk": "^1.1.1",
    "connect-history-api-fallback": "^1.1.0",
    "cssnano": "^3.4.0",
    "eslint": "^1.10.3",
    "eslint-plugin-react": "^3.14.0",
    "esprima-fb": "^15001.1001.0-dev-harmony-fb",
    "gulp": "^3.9.0",
    "gulp-debug": "^2.1.2",
    "gulp-eslint": "^1.1.1",
    "gulp-if": "^2.0.0",
    "gulp-image-resize": "^0.7.1",
    "gulp-imagemin": "^2.4.0",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^6.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-template": "^3.1.0",
    "gulp-uglify": "^1.5.1",
    "history": "^1.17.0",
    "imagemin-webp": "^3.1.0",
    "karma": "^0.13.19",
    "karma-browserify": "^4.4.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "mocha": "^2.3.4",
    "postcss-bem-linter": "^2.3.0",
    "postcss-calc": "^5.2.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-custom-properties": "^5.0.0",
    "postcss-easings": "^0.3.0",
    "postcss-import": "^7.1.3",
    "postcss-nested": "^1.0.0",
    "postcss-url": "^5.0.2",
    "sitemap": "^1.5.0",
    "stylelint": "^3.2.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "yargs": "^3.31.0"
  },
  "browser": {},
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "stage-0",
            "react"
          ]
        }
      ]
    ]
  },
  "paths": {
    "connect": {
      "dirMin": "./",
      "dir": "dist",
      "files": [
        "dist/**/*",
        "index.html"
      ]
    },
    "html": {
      "entry": "src/index.html",
      "dest": "dist",
      "destMin": ""
    },
    "images": {
      "entry": "src/img/png",
      "dest": "dist/img"
    },
    "scripts": {
      "entry": "src/index.js",
      "dest": "dist/js",
      "modernizr": {
        "entry": "src/utils/modernizr.js",
        "dest": "dist/js"
      },
      "lint": [
        "src/**/*.js",
        "tasks/**/*.js",
        "!src/utils/modernizr.js"
      ]
    },
    "styles": {
      "entry": "src/index.css",
      "dest": "dist/css/",
      "watch": [
        "src/**/*.css"
      ],
      "lint": [
        "src/**/*.css",
        "!src/font/**/*.css"
      ]
    }
  }
}
